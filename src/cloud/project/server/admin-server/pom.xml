            <artifactId>sequoiacm-infrastructure-server-common</artifactId>
        </dependency>

        <dependency>
            <groupId>com.sequoiadb</groupId>
            <artifactId>sequoiacm-infrastructure-statistics-common</artifactId>
        </dependency>
        <dependency>
